#include <iostream>
using namespace std;

int main() {
    int a[] = { 5, 8, 7, 12, 14, 3, 9} ;
    int x =2, k = 8;
    int flag=0,count=0;
    
    for(int i=0;i<sizeof(a)/sizeof(a[0]);i=i+x)
    {
        count+=1;
       for(int j=i;j<x+i;j++)
       {
           
           if(a[j]==k)
           {
               flag+=1;
               break;
           }
       }
       
       
    }
    if(flag==count)
    cout<<"yes";
	else
	cout<<"no"<<count;
	return 0;
}
